{
  "projectType": "library",
  "sourceRoot": "libs/akita-store/src",
  "prefix": "golden-state-management",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/akita-store"],
      "options": {
        "jestConfig": "libs/akita-store/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/akita-store/**/*.ts",
          "libs/akita-store/**/*.html"
        ]
      }
    }
  },
  "tags": ["store"]
}
